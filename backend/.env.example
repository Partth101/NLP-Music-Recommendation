# MoodTune AI Backend Environment Variables

# Application
APP_NAME=MoodTune AI
DEBUG=false

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/moodtune

# Redis
REDIS_URL=redis://localhost:6379

# JWT Authentication
JWT_SECRET_KEY=your-super-secret-key-change-in-production
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# ML Model
MODEL_PATH=app/ml/savedmodel
MODEL_VERSION=bert-emotion-v1.0

# Spotify API (optional)
SPOTIFY_CLIENT_ID=your-spotify-client-id
SPOTIFY_CLIENT_SECRET=your-spotify-client-secret

# CORS
CORS_ORIGINS=["http://localhost:3000","https://moodtune.vercel.app"]
